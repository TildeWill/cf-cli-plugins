#!/usr/bin/env ruby

fork do
  `afplay #{File.dirname(__FILE__)}/push-it.mp3`
end

puts " ______   __  __   ______   ___   ___       ________  _________ "
puts "/_____/\\ /_/\\/_/\\ /_____/\\ /__/\\ /__/\\     /_______/\\/________/\\"
puts "\\:::_ \\ \\\\:\\ \\:\\ \\\\::::_\\/_\\::\\ \\\\  \\ \\    \\__.::._\\/\\__.::.__\\/"
puts " \\:(_) \\ \\\\:\\ \\:\\ \\\\:\\/___/\\\\::\\/_\\ .\\ \\      \\::\\ \\    \\::\\ \\  "
puts "  \\: ___\\/ \\:\\ \\:\\ \\\\_::._\\:\\\\:: ___::\\ \\     _\\::\\ \\__  \\::\\ \\ "
puts "   \\ \\ \\    \\:\\_\\:\\ \\ /____\\:\\\\: \\ \\\\::\\ \\   /__\\::\\__/\\  \\::\\ \\"
puts "    \\_\\/     \\_____\\/ \\_____\\/ \\__\\/ \\::\\/   \\________\\/   \\__\\/"
puts ""

system("cf push #{ARGV.join(' ')}")

`killall afplay`
